{
  "name": "ISEPStream",
  "version": "1.0.0",
  "author": "José Monteiro | Gonçalo Silva :: ISEP",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "build::electron": "ionic build && electron .",
    "build::prod::electron": "ionic build --prod && npx electron .",
    "package::mac": "ionic build --prod && electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=src/assets/native/icon.icns --prune=true --out=release-builds",
    "package::win": "ionic build --prod && electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --icon=src/assets/native/icon.ico --prune=true --out=release-builds",
    "package::linux": "ionic build --prod && electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer::mac": "electron-installer-dmg release-builds/ISEPStream app-darwin-x64/ISEPStream app.app isep-stream-app --out=release-builds --overwrite --icon=src/assets/native/icon.icns",
    "create-installer::win": "node installers/windows/createinstaller.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@cycjimmy/jsmpeg-player": "^5.0.1",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "browser-env": "^3.3.0",
    "custom-electron-titlebar": "^3.2.5",
    "dom-to-image": "^2.6.0",
    "dom-to-image-more": "^2.8.0",
    "electron-acrylic-window": "^0.5.4",
    "electron-json-storage": "^4.3.0",
    "electron-peer-connection": "^1.0.6",
    "html2canvas": "^1.0.0-rc.7",
    "jsmpeg": "^1.0.0",
    "jsmpeg-player": "^3.0.3",
    "ngx-color": "^6.2.0",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.5",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "windows10-electron-vibrancy": "^1.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.7",
    "codelyzer": "^6.0.0",
    "electron": "^11.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "browser": {
    "child_process": false,
    "crypto": false
  }
}
